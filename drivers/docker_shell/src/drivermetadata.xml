<Driver Description="This driver orchestrate all the command that will be executed on Docker" MainClass="driver.DockerShellDriver" Name="Docker Shell Driver" Version="0.1.32">
    <Layout>
        <Category Name="Deployment">
            <Command Description="" DisplayName="Deploy Container" EnableCancellation="true" Name="deploy_container" Tags="allow_unreserved" />
        </Category>
        <Category Name="Power">
            <Command Description="" DisplayName="Power On" Name="PowerOn" Tags="power" />
            <Command Description="" DisplayName="Power Off" Name="PowerOff" Tags="power" />
            <Command Description="" DisplayName="Power Cycle" Name="PowerCycle" Tags="power" />
        </Category>
        <Category Name="Hidden Commands">
            <Command Description="" DisplayName="Delete VM Only" Name="destroy_vm_only" Tags="remote_app_management,allow_shared" />
        </Category>
    </Layout>
</Driver>